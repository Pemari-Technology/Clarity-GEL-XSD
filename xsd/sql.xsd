            <xs:attribute name="context" type="xs:string" />
            <xs:attribute name="dataSource" type="xs:string" />
            <xs:attribute name="isolation" type="xs:string" />
        </xs:complexType>
    </xs:element>

    <xs:element name="update">
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:element name="dateParam" type="sql:dateParamType" minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="param" type="sql:paramType" minOccurs="0" maxOccurs="unbounded" />
                <xs:any processContents="lax" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
            <xs:attribute name="escapeText" type="xs:boolean" />
            <xs:attribute name="trim" type="xs:boolean" />
            <xs:attribute name="context" type="xs:string" />
            <xs:attribute name="dataSource" type="xs:string" />
            <xs:attribute name="scope" type="xs:string" />
            <xs:attribute name="sql" type="xs:string" />
            <xs:attribute name="var" type="xs:string" />
        </xs:complexType>
    </xs:element>

</xs:schema>
